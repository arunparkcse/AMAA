import { Component, OnInit } from '@angular/core';
<% schema.entities.forEach(ent => { %>
import { <%= toPascal(ent.name) %>Service } from '../../features/<%= toKebab(ent.name) %>/<%= toKebab(ent.name) %>.service';
<% }) %>

@Component({
  selector: 'app-dashboard',
  templateUrl: './dashboard.component.html'
})
export class DashboardComponent implements OnInit {
  <% schema.entities.forEach(ent => { %>
  <%= toCamel(ent.name) %>Count = 0;
  <% }) %>

  constructor(
    <% schema.entities.forEach(ent => { %>
    private <%= toCamel(ent.name) %>Service: <%= toPascal(ent.name) %>Service,
    <% }) %>
  ) {}

  ngOnInit() {
    <% schema.entities.forEach(ent => { %>
    this.<%= toCamel(ent.name) %>Service.getAll().subscribe(res => {
      this.<%= toCamel(ent.name) %>Count = (res.data?.<%= toCamel(ent.name) %>s || res).length;
    });
    <% }) %>
  }
}