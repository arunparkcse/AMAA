<nb-card>
  <nb-card-header>Dashboard</nb-card-header>
  <nb-card-body>
    <p>Welcome to the admin panel!</p>
    <div class="stats">
      <% entities.forEach(ent => { %>
      <nb-card size="tiny">
        <nb-card-body><%= toPascal(ent.name) %> Count: {{ <%= toCamel(ent.name) %>Count }}</nb-card-body>
      </nb-card>
      <% }) %>
    </div>
  </nb-card-body>
</nb-card>