import { Component } from '@angular/core';
<% if (schema.adminPanel) { %>
import { NbMenuItem } from '@nebular/theme';
<% } %>

@Component({
  selector: 'app-root',
  templateUrl: './app.component.html'
})
export class AppComponent {
  <% if (schema.adminPanel) { %>
  menuItems: NbMenuItem[] = [
    { title: 'Dashboard', link: '/pages/dashboard', icon: 'home-outline' },
    <% schema.entities.forEach(ent => { %>
    { title: '<%= toPascal(ent.name) %>', link: '/pages/<%= toKebab(ent.name) %>', icon: 'grid-outline' },
    <% }) %>
    { title: 'Logout', link: '/login', icon: 'log-out-outline' }
  ];
  <% } %>
}