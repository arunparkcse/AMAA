import { Router } from 'express';
import authRoutes from './auth';
<% entities.forEach(ent => { %>
import <%= toCamel(ent.name) %>Routes from './<%= toKebab(ent.name) %>';
<% }) %>

const router = Router();
router.use('/auth', authRoutes);
<% entities.forEach(ent => { %>
router.use('/<%= toKebab(ent.name) %>', <%= toCamel(ent.name) %>Routes);
<% }) %>

export default router;